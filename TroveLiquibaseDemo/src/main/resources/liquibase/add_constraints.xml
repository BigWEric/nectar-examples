<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <changeSet id="3" author="mpaulo">
        <addUniqueConstraint columnNames="CODE,DESCRIPTION"
                             constraintName="CODE_DESC_UNIQUE"
                             tableName="PRODUCTS"/>
        <addForeignKeyConstraint baseColumnNames="PRODUCT_CODE"
                                 baseTableName="PRODUCT_CELLAR"
                                 constraintName="PRODUCT_FK"
                                 referencedColumnNames="CODE"
                                 referencedTableName="CELLARS"/>
    </changeSet>
</databaseChangeLog>